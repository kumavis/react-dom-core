{
  "env": {
    "jest": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:ft-flow/recommended"],
  "parser": "hermes-eslint",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["ft-flow", "prettier"],
  "rules": {
    "no-console": "error",
    "no-debugger": "error",
    "prettier/prettier": "error"
  }
}
