  },
  "keywords": [
    "react-property",
    "html",
    "svg",
    "dom",
    "property",
    "attribute",
    "config",
    "react",
    "react-dom"
  ],
  "files": [
    "/lib"
  ],
  "devDependencies": {
    "eslint": "^6.0.1",
    "eslint-plugin-prettier": "^3.1.0",
    "react-dom": "^15",
    "prettier": "^1.17.1"
  },
  "license": "MIT"
}
