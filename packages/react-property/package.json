  },
  "keywords": [
    "react-property",
    "html",
    "svg",
    "dom",
    "property",
    "attribute",
    "config",
    "react",
    "react-dom"
  ],
  "files": [
    "/lib"
  ],
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.1.0",
    "prettier": "^2.3.2",
    "react-dom": "^15"
  },
  "license": "MIT"
}
